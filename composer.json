{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6.2",
        "guzzlehttp/guzzle": "^7.5.1",
        "intervention/image": "^2.7.1",
        "laravel/framework": "^10.9",
        "laravel/sanctum": "^3.2",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2.1",
        "maatwebsite/excel": "^3.1.40",
        "milon/barcode": "^10.0.0",
        "mpdf/mpdf": "^8.1",
        "niklasravnsborg/laravel-pdf": "^4.1",
        "nwidart/laravel-modules": "^10.0.0",
        "picqer/php-barcode-generator": "^2.2.2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-permission": "^5.5",
        "twilio/sdk": "^6.44",
        "yajra/laravel-datatables": "^9.0.0",
        "yajra/laravel-datatables-buttons": "^9.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.1.0",
        "barryvdh/laravel-debugbar": "^3.8.1",
        "kitloong/laravel-migrations-generator": "^6.8.0",
        "thedoctor0/laravel-factory-generator": "^1.3.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
